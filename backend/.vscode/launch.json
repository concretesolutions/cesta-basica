{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/main.js",
      "runtimeArgs": [
        "-r",
        "esm"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "load-user",
      "program": "${workspaceFolder}/src/load/user.js",
      "runtimeArgs": [
        "-r",
        "esm"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}